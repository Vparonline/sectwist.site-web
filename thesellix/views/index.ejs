    <script>
      let avatarStatusElement = document.querySelector('.avatar-status-online');
      let idleTimeout;
      
      document.addEventListener('mousemove', () => {
        clearTimeout(idleTimeout);
        
        if (avatarStatusElement.classList.contains('avatar-status-away')) {
          avatarStatusElement.classList.remove('avatar-status-away');
        }
        
        idleTimeout = setTimeout(() => {
          avatarStatusElement.classList.add('avatar-status-away');
        }, 30000);
      });
      
      document.addEventListener('mouseleave', () => {
        if (!avatarStatusElement.classList.contains('avatar-status-online')) {
          avatarStatusElement.classList.add('avatar-status-online');
        }
      });
      </script>
